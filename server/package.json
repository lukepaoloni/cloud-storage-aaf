{
  "name": "cloud-storage",
  "version": "0.0.1",
  "description": "A cloud storage solution for media files.",
  "author": "Luke Paoloni",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --watch --config ./test/jest-e2e.json",
    "seed": "nps db.seed"
  },
  "dependencies": {
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/jwt": "^0.2.1",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/typeorm": "^5.2.2",
    "@types/algoliasearch": "^3.30.3",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "algoliasearch": "^3.32.0",
    "awesome-typescript-loader": "^5.2.1",
    "bcryptjs": "^2.4.3",
    "chai": "^4.2.0",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "dotenv": "^6.2.0",
    "faker": "^4.1.0",
    "filereader": "^0.10.3",
    "image-downloader": "^3.4.2",
    "jsonwebtoken": "^8.4.0",
    "mkdirp": "^0.5.1",
    "mocha": "^6.0.0",
    "moment": "^2.24.0",
    "mongodb": "^3.1.10",
    "multer": "^1.4.1",
    "nps": "^5.9.3",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "ts-transformer-keys": "^0.3.4",
    "typeorm": "^0.2.11",
    "typeorm-seeding": "^1.0.0-beta.6",
    "typescript": "^3.0.1",
    "underscore": "^1.9.1",
    "webpack": "^4.29.5",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.16.0",
    "@types/faker": "^4.1.4",
    "@types/glob": "^7.1.1",
    "@types/jest": "^23.3.1",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/mkdirp": "^0.5.2",
    "@types/multer": "^1.3.7",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "@types/underscore": "^1.8.9",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "webpack-cli": "^3.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
